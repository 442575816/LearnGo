<?xml version="1.0" encoding="UTF-8" ?>
<servlet>
    <init-param>
        <props>
            <property name="compress" type="bool">false</property>
            <property name="sessionTimeout" type="int">100000</property>
        </props>
    </init-param>
    <network>
        <init-param>
            <props>
                <property name="port" type="int">8010</property>
                <property name="httpport" type="int">8011</property>
            </props>
        </init-param>
        <tcp-param>
            <props>
                <property name="reuseAddress" type="boolean">true</property>
            </props>
        </tcp-param>
    </network>
</servlet>
    
